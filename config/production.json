{
  "ORM_CONFIG": {
    "database": "/data/theta-data/database/data.sqlite",
    "synchronize": true,
    "autoLoadEntities": true,
    "name": "THETA_DATA",
    "type": "sqlite",
    "entities": ["src/**/*.entity{.ts,.js}"],
    "bigNumberStrings": false,
    "logging": false,
    "extra": {
      "charset": "utf8mb4_unicode_ci"
    }
  },
  "THETA_NODE_HOST" : "http://ip-172-31-35-81.ap-southeast-1.compute.internal/rpc",
  "LOG_PATH": "/home/ubuntu/theta-data/logs/",
  "ANALYSE_SLEEP" : 20,
  "REDIS": {
    "host": "127.0.0.1",
    "port": 6379
  },
  "RATE_LIMIT": {
    "ttl": 60,
    "limit": 60
  }
}
